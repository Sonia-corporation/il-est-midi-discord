    "jest-ts-auto-mock": "2.0.0",
    "lint-staged": "11.0.1",
    "madge": "5.0.1",
    "majestic": "1.8.1",
    "node-notifier": "10.0.0",
    "parcel-bundler": "1.12.5",
    "prettier": "2.2.1",
    "pug": "3.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "shx": "0.3.3",
    "stryker-cli": "1.0.2",
    "ts-auto-mock": "3.3.0",
    "ts-jest": "27.0.3",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.10.1",
    "ttypescript": "1.5.12",
    "typescript": "4.3.5",
    "weak-napi": "2.0.2"
  }
}
